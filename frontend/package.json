{
  "name": "vs-dotcom-frontend",
  "version": "0.1.0",
  "description": "visitscotland.com Frontend",
  "author": "lukenofurther",
  "main": "dist/library/manifest.json",
  "type": "module",
  "files": [
    "dist/library"
  ],
  "homepage": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "cd .. && husky install ./custom-hooks",
    "system": "cross-env NODE_ENV=development node ./build/system server --remote-profile hippo",
    "system:build": "cross-env NODE_ENV=production node ./build/system build",
    "library": "cross-env NODE_ENV=production npx webpack --config ./build/library.webpack.conf.js",
    "library:dev": "cross-env NODE_ENV=development npx webpack --config ./build/library.webpack.conf.js",
    "ssr": "npm-run-all ssr:build ssr:serve",
    "ssr:build": "npm-run-all ssr:build:client ssr:build:server",
    "ssr:build:dev": "cross-env NODE_ENV=development yarn ssr:build",
    "ssr:serve": "nodemon ./ssr/server/index.js",
    "ssr:serve:dev": "cross-env NODE_ENV=development yarn ssr:serve",
    "ssr:build:client": "npx webpack --config ./build/ssr.client.webpack.conf.js",
    "ssr:build:server": "npx webpack --config ./build/ssr.server.webpack.conf.js",
    "lint": "eslint",
    "lint:all": "yarn lint:src && yarn lint:build && yarn lint:cypress",
    "lint:src": "yarn lint \"src/**/*.{js,vue}\"",
    "lint:fix": "yarn lint \"src/**/*.{js,vue}\" --fix",
    "lint:watch": "esw \"src/**/*.{js,vue}\" --watch --ext .js,.vue --watch-delay 0 --fix",
    "lint:style": "stylelint",
    "lint:style:all": "lint:style \"src/**/*.{vue,scss}\"",
    "lint:build": "yarn lint \"build/*.{js,vue}\"",
    "lint:build:fix": "yarn lint \"build/*.{js,vue}\" --fix",
    "lint:cypress": "yarn lint \"cypress/**/*.{js,vue}\"",
    "test": "jest --config ./test/unit/jest.conf.js",
    "test:silent": "jest --config ./test/unit/jest.conf.js --reporters jest-silent-reporter",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:debug:chrome": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand --config ./test/unit/jest.conf.js",
    "test:e2e": "cypress open",
    "theo": "theo ./src/tokens/tokens.yml --transform web --format map.scss,scss,raw.json,json --dest ./src/assets/tokens",
    "theo:watch": "onchange \"./src/tokens/*.yml\" -- npm run _theo",
    "presystem": "yarn theo",
    "presystem:build": "yarn theo",
    "pressr:build": "yarn theo",
    "pretest": "yarn theo",
    "pretest:watch": "yarn theo",
    "pretest:coverage": "yarn theo",
    "pretest:debug:chrome": "yarn theo",
    "pretest:e2e": "yarn theo"
  },
  "repository": {
    "type": "git",
